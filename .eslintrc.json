{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "SyntheticEvent": true
  },
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "rules": {
    "newline-after-var": "error",
    "newline-before-return": "error"
  }
}
